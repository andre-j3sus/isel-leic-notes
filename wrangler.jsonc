{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "isel-leic-notes",
  "main": "./dist/_worker.js/index.js",
  "compatibility_date": "2025-01-30",
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "directory": "./dist",
    "binding": "ASSETS"
  },
  "observability": {
    "enabled": true
  },
  "routes": [
    {
      "pattern": "isel-leic-notes.andrejesus.com",
      "custom_domain": true
    }
  ]
}
